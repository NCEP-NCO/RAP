#!/bin/sh
set -xa

export RUN_ENVIR=${RUN_ENVIR:-prod}
export SENDSMS=${SENDSMS:-YES}

export RUN=${RUN:-rap}
export model=${model:-rap}
export model_ver=${model_ver:-v2.0.0}

if [ "$RUN_ENVIR" != prod ]      ### For Developers, "group_name" is passed from the SMS script
then
  group_name=${group_name:-meso}
  package_name=${package_name:-rap}
#  . /${group_name}/save/${LOGNAME}/${package_name}/nwprod/parm/${package_name}_para_config
  export userid=${userid:-$LOGNAME}
  export DATA_IN=${DATA_IN:-/ptmpd1/$userid}
fi

####################################
# obtain unique process id (pid) and make temp directory
####################################
export pid=$$
export DATA_IN=${DATA_IN:-/ptmpd1/Corey.Guastini}
export DATA=$DATA_IN/test
mkdir -p $DATA
cd $DATA

export cycle=t${cyc}z
#export tmmark=tm00

####################################
# Specify Execution Areas
####################################
export HOMErap=${HOMErap:-/meso/save/Corey.Guastini/nw${envir}/${RUN}.${model_ver}}
export EXECrap=${EXECrap:-${HOMErap}/exec}
export FIXrap=${FIXrap:-${HOMErap}/fix}
export PARMrap=${PARMrap:-${HOMErap}/parm}
export USHrap=${USHrap:-${HOMErap}/ush}

###################################
# Set up the UTILITIES
###################################
export utilscript=${utilscript:-/nwprod/util/ush}
export utilexec=${utilexec:-/nwprod/util/exec}

###############################################################
# Run setup to initialize working directory and utility scripts
###############################################################
sh $utilscript/setup.sh

###########################################
# Run setpdy and initialize PDY variables
###########################################
sh $utilscript/setpdy.sh
. PDY
#PDY=20130517

##############################################
# Define COM directories
##############################################

export COM_IN=${COM_IN:-/ptmpd1/Corey.Guastini/com/${RUN}/${envir}}
export COM_OUT=${COM_OUT:-/ptmpd1/Corey.Guastini/com/${RUN}/${envir}}

export COM=${COM:-COM_IN}
export COMIN=${COMIN:-${COM_IN}/${RUN}.${PDY}}
export COMOUT=${COMOUT:-${COM_OUT}/${RUN}.${PDY}}

mkdir -m 775 -p $COMOUT 

##############################################
# Define GES directories
##############################################
export gespath=${gespath:-/ptmpd1/Corey.Guastini/nwges/${envir}}
env

export FCSTDIR=$DATA_IN/rap_fcst_${envir}_${cyc}
########################################################
# Execute the script.
${RAP_PRDGENMGRSH:-$HOMErap/scripts/exrap_test.sh.sms}
########################################################
exit
fi
